{
    "name": "@eggziom/geek-regime-js-ui-kit",
    "version": "1.0.0",
    "description": "UI kit for web client",
    "author": "Alexey Soloviev",
    "license": "UNLICENSED",
    "main": "dist/main/index.js",
    "module": "dist/main/index.js",
    "types": "dist/main/index.d.ts",
    "engines": {
        "npm": ">=10.0.0 <11.0.0",
        "node": ">=18.0.0 <19.0.0"
    },
    "type": "module",
    "sideEffects": [
        "**/*.css",
        "**/*.scss",
        "**/*.d.ts"
    ],
    "exports": {
        "./*": {
            "types": "./dist/*/index.d.ts",
            "import": "./dist/*/index.js",
            "default": "./dist/*/index.js"
        }
    },
    "scripts": {
        "start": "npm run storybook",
        "storybook": "storybook dev -c tools/storybook",
        "build": "vite build --config tools/vite.config.ts",
        "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest --config=tools/jest.config.js",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "npm run test -- --coverage",
        "test:coverage:watch": "npm run test:coverage -- --watchAll",
        "test:clear": "npm run test -- --clearCache"
    },
    "peerDependencies": {
        "@eggziom/geek-regime-js-configs": "1",
        "@eggziom/geek-regime-js-utils": "1",
        "react": "18",
        "react-dom": "18"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/plugin-proposal-decorators": "7.22.6",
        "@babel/preset-react": "7.22.5",
        "@babel/preset-typescript": "7.22.5",
        "@eggziom/geek-regime-js-configs": "1.0.0",
        "@eggziom/geek-regime-js-utils": "1.0.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "15.0.6",
        "@types/lodash.debounce": "4.0.7",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/react-router-dom": "5.3.3",
        "@types/styled-components": "5.1.26",
        "@types/webpack-bundle-analyzer": "4.6.0",
        "core-js": "3.31.0",
        "jest-environment-jsdom": "29.5.0",
        "lodash.debounce": "4.0.8",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.14.1",
        "styled-components": "5.3.5",
        "webpack-bundle-analyzer": "4.9.0"
    }
}
