{
    "name": "@eggziom/geek-regime-js-ui-kit",
    "version": "0.0.22",
    "description": "UI kit for web client",
    "author": "Alexey Soloviev",
    "license": "UNLICENSED",
    "main": "dist/main/index.js",
    "types": "dist/main/index.d.ts",
    "sideEffects": [
        "**/*.css",
        "**/*.scss",
        "**/*.d.ts"
    ],
    "scripts": {
        "start": "npm run storybook",
        "storybook": "start-storybook -c configs/storybook",
        "build": "npm run build:webpack && npm run build:main",
        "build:webpack": "webpack --mode=production --config configs/webpack.config.ts",
        "build:main": "tsc && tsc-alias",
        "prebuild": "rimraf dist",
        "prepare": "npm run build",
        "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest --config=configs/jest.config.ts",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "npm run test -- --coverage",
        "test:coverage:watch": "npm run test:coverage -- --watchAll",
        "test:clear": "npm run test -- --clearCache"
    },
    "peerDependencies": {
        "@eggziom/geek-regime-js-commons": "0",
        "@eggziom/geek-regime-js-configs": "0",
        "date-fns": "2",
        "react": "18",
        "react-dom": "18",
        "styled-components": "5"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "7.18.6",
        "@babel/plugin-proposal-decorators": "7.22.6",
        "@babel/preset-env": "7.22.6",
        "@babel/preset-react": "7.22.5",
        "@babel/preset-typescript": "7.22.5",
        "@eggziom/geek-regime-js-commons": "0.0.24",
        "@eggziom/geek-regime-js-configs": "0.0.4",
        "@storybook/addon-a11y": "6.5.16",
        "@storybook/addon-essentials": "6.5.16",
        "@storybook/addon-interactions": "6.5.16",
        "@storybook/builder-webpack5": "6.5.16",
        "@storybook/manager-webpack5": "6.5.16",
        "@storybook/react": "6.5.16",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.2",
        "@types/node": "18.15.0",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/react-router-dom": "5.3.3",
        "@types/styled-components": "5.1.26",
        "@types/webpack": "5.28.1",
        "@types/webpack-bundle-analyzer": "4.6.0",
        "@types/webpack-node-externals": "3.0.0",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/parser": "5.27.1",
        "babel-loader": "9.1.2",
        "babel-plugin-styled-components": "2.1.4",
        "clean-webpack-plugin": "4.0.0",
        "core-js": "3.31.0",
        "css-loader": "6.8.1",
        "eslint": "8.17.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "5.11.0",
        "eslint-plugin-unicorn": "42.0.0",
        "fork-ts-checker-webpack-plugin": "8.0.0",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "mini-css-extract-plugin": "2.7.6",
        "postcss-preset-env": "9.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.14.1",
        "rimraf": "5.0.1",
        "sass": "1.63.6",
        "sass-loader": "13.3.2",
        "styled-components": "5.3.11",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsc-alias": "1.8.7",
        "typescript": "4.7.4",
        "webpack": "5.88.1",
        "webpack-bundle-analyzer": "4.9.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-merge": "5.9.0",
        "webpack-node-externals": "3.0.0"
    }
